<?xml version="1.0" encoding="utf-8" ?>
<ContentView
    x:Class="ForestalCasablancaApp.Controls.NumericEntryCell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:models="clr-namespace:ForestalCasablancaApp.Models">
    <Grid>
        <Border Style="{StaticResource EntryCellBorderStyle}" />
        <Entry
            x:Name="Input"
            Margin="0,0,5,0"
            Keyboard="Numeric"
            WidthRequest="50" />
    </Grid>
</ContentView>
